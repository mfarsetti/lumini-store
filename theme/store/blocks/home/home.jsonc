  "rich-text#sobre-a-lumini-titulo": {
    "title": "Home | Sobre a Lumini - Título",
    "props": {
      "blockClass": ["sobre-a-lumini-titulo"],
      "font": "t-body",
      "text": "a lumini",
      "textAlignment": "CENTER",
      "textColor": "c-on-base",
      "textPosition": "CENTER"
    }
  },
  "rich-text#sobre-a-lumini-texto": {
    "title": "Home | Sobre a Lumini - Texto",
    "props": {
      "blockClass": ["sobre-a-lumini-texto"],
      "font": "t-body",
      "text": "somos uma empresa brasileira de iluminação, nascida e criada na linguagem de arquitetos e lighting designers. <br> acreditamos na importância da iluminação para o bem- estar das pessoas e há 35 anos nos dedicamos a melhorar a qualidade de vida de nossos clientes.<p>nossos produtos são pautados pelo design, pela simplicidade e pela tecnologia.</p>",
      "textAlignment": "CENTER",
      "textColor": "c-on-base",
      "textPosition": "CENTER"
    }
  },

  "info-card#info-card-colecao": {
    "title": "Home | Info Card 1",
    "props": {
      "id": "home__infoCard",
      "isFullModeStyle": true,
      "textPosition": "left",
      "imageUrl": "assets/img/inforcard-home.png",
      "mobileImageUrl": "assets/img/inforcard-home-mobile.png",
