  "flex-layout.row#sobre-a-lumini": {
    "title": "Home | Sobre a Lumini",
    "children": [
      "rich-text#sobre-a-lumini-titulo",
      "rich-text#sobre-a-lumini-texto"
    ],
    "props": {
      "blockClass": ["home__sobre"]
    }
