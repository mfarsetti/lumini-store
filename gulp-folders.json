  "options": {
    "sass": {
      "includePaths": [
        "sass"
      ],
      "outputStyle": "compressed"
    },
    "image": {
      "key": "rLgKLJKhNxpbcjk1xzcZ9CmP0LYrrhKh",
      "log": false
    }
  }
}
