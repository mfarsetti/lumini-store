{
  "paths": {
    "sass": {
      "src": "sass/**/*.scss",
      "dest": {
        "theme": "theme/styles/css",
        "checkout": "checkout-ui-settings/checkout-ui-custom"
      }
    },
    "image": {
      "src": "theme/assets/img/**/*.{png,jpg,jpeg}",
      "dest": "theme/assets/img"
    }
